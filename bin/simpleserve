var options = program2.opts();
var port = Number.parseInt(options.port);
var directory = options.dir;
var app = new Hono2;
app.use("*", logger());
app.use("/*", serveStatic({ root: directory }));
app.notFound((c) => {
  return c.text("404 Not Found", 404);
});
serve({
  fetch: app.fetch,
  port
}, () => {
  console.log(source_default.green(`
\uD83D\uDE80 Server started!`));
  console.log(source_default.blue(`
\uD83D\uDCC1 Serving directory: ${source_default.yellow(directory)}`));
  console.log(source_default.blue(`\uD83D\uDD17 Local: ${source_default.yellow(`http://localhost:${port}`)}
`));
});
process.on("SIGINT", () => {
  console.log(source_default.red(`
\uD83D\uDC4B Shutting down server...`));
  process.exit(0);
});
